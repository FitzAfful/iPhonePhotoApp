platform :ios, '11.0'
use_frameworks!
inhibit_all_warnings!

target 'iPhonePhotoApp' do
    pod 'Nuke'
    pod 'Alamofire'
    pod 'RealmSwift'
    pod 'SwiftLint'
    pod 'Mocker'
		
    target 'iPhonePhotoAppTests' do
        inherit! :search_paths
    end
end

